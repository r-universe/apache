[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/apache.r-universe.dev
	branch = HEAD
[submodule "arrow"]
	path = arrow
	url = https://github.com/apache/arrow
	branch = r-universe-release
	subdir = r
[submodule "nanoarrow"]
	path = nanoarrow
	url = https://github.com/apache/arrow-nanoarrow
	subdir = r
